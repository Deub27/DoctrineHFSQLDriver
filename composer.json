{
    "name": "tbcd/doctrine-hfsql-driver",
    "type": "library",
    "description": "Doctrine HFSQL Driver with ODBC",
    "authors": [
        {
            "name": "Thomas Beauchataud"
        }
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.0",
    "require": {
        "php": ">=8.0.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-odbc": "*",
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "TBCD\\Doctrine\\HFSQLDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TBCD\\Doctrine\\HFSQLDriver\\Tests\\": "tests/"
        }
    }
}